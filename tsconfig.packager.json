{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2015",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@common/*": ["./common/*"]
    },
    "lib": ["WebWorker", "DOM"],
    "typeRoots": ["./node_modules/@types", "./typings"]
  }
}
